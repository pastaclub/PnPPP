{
  "baseDir": "/Users/dennis/Dropbox/PCB",
  "bom": {
    "filePattern": "(.*\/)Outputs/BOM\/BOM_PartType-([^.]*).csv",
    "outputFileName": "bom.csv",
    "jsonFileName": "bom.json",
    "columns": {
      "Comment": "Comment",
      "Designator": "Designator",
      "Footprint": "Footprint",
      "LCSC": "LCSC Part #"
    }
  },
  "pnp": {
    "filePattern": "(.*\/)Outputs/Pick Place\/Pick Place for ([^.]*).csv",
    "outputFileName": "pnp.csv",
    "columns": {
      "Designator": "Designator",
      "Mid X": "Mid X",
      "Mid Y": "Mid Y",
      "Layer": "Layer",
      "Rotation": "Rotation"
    }
  },
  "gerber": {
    "filePattern": "((.*\/))(Outputs/((Gerber)|(NC Drill))\/([^.]*).([^.]*))",
    "archiveName": "gerber.zip",
    "folders": ["Outputs/Gerber", "Outputs/NC Drill"]
  },
  "autoRotation": {
    "enabled": true,
    "parameter": "JlcRotation"
  },
  "panelization": {
    "designator": "PnPPP_Panel",
    "xName": "Mid X",
    "yName": "Mid Y"
  }
}